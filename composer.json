{
    "name": "slimphp-api/slim-mvc",
    "description": "A module for slim-api for mvc, creating structure, generators for creating controllers/models and scaffold",
    "require": {
        "php": ">5.6",
        "slimphp-api/slim-api": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mikey179/vfsStream": "1.*",
        "satooshi/php-coveralls": "^0.7.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"SlimApi\\Mvc\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApi\\MvcTest\\": "tests/phpunit/"}
    }
}
